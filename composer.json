{
    "name": "iiifx-production/lazy-init",
    "type": "extension",
    "description": "LazyInit - lazy initialization helper",
    "keywords": [
        "lazy-init",
        "lazy",
        "php"
    ],
    "homepage": "https://github.com/iiifx-production/lazy-init",
    "license": "MIT License",
    "authors": [
        {
            "name": "Vitaliy IIIFX Khomenko",
            "email": "iiifx@yandex.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*@stable"
    },
    "autoload": {
        "psr-4": {
            "iiifx\\LazyInit\\": "source/"
        }
    },
    "scripts": {
        "test": "phpunit tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
